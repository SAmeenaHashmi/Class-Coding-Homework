#include <stdio.h>

// program that keeps taking user input until 0 is inputted. Then it outputs the largest entered number

int main() {
    // Write C code here
    int input;
    int largest = 0;
    do {
        scanf("%d", &input);
        if (input > largest) {
            largest = input;
        }
    } while (input !=0);
printf("%d", largest);
    return 0;
}
